            } else if (ua.indexOf("android") > -1 || ua.indexOf("windows phone") > -1 || ua.indexOf("blackberry") > -1) {
                url = "sms:" + dauso + "?body=" + sms;
                location.href = url
            } else alert("Soan " + sms + " gui " + dauso)
        }
